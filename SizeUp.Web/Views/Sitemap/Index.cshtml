@{
    Layout = null;
}<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    @foreach(var x  in ViewBag.Community){
   <sitemap>
      <loc>http://@SizeUp.Core.Web.WebContext.Current.Domain/sitemap/@x.State/@x.County/@x.City/</loc>
   </sitemap>
    }
      @foreach (var x in ViewBag.IndustryCommunity)
      {
   <sitemap>
      <loc>http://@SizeUp.Core.Web.WebContext.Current.Domain/sitemap/@x.State/@x.County/@x.City/@x.Industry/</loc>
   </sitemap>
    }
</sitemapindex>
