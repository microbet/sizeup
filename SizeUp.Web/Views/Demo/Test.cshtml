@section scripts{

    <script type="text/javascript">
        var done = function(){
            var opts = {
                currentInfo: @Html.Raw(ViewBag.CurrentInfoJSON),
                isAuthenticated: @(HttpContext.Current.User.Identity.IsAuthenticated ? "true" : "false")
                };
            var driver = new sizeup.views.dashboard.dashboard(opts);
        };

        var notifier = new sizeup.core.notifier(done);
        var apiLoaded = notifier.getNotifier();
        var doc = notifier.getNotifier();
        $(document).ready(doc);
    </script>
}



<style>
    #header ul li a.disabled { background-color: #F7D6AA; }
    #header ul li a { display: block; padding: 0.5em; text-decoration: none; }
    a { color: #e78500; text-decoration: none; font-weight: bold; }
    #header { background-color: #F8F8F8; border-bottom: 2px solid #EEEEEE; height: 2.6em; margin-top: 0.9em; }
    #header ul { position: relative; text-align: right; }
    #header ul li { display: inline-block; vertical-align: top; }
    #header ul li.dashboard { float: left; }
    #header ul li.competition { float: left; }
    #header ul li.advertising { float: left; }
    #header ul li.help { top: 0.45em; position: relative; }
    #header ul li a.button.help { background-color: #6699AA; border-radius: 4em 4em 4em 4em; display: inline; font-size: 1em; padding: 0.1em 0.7em; position: relative; }

    a.button,
    .button { color: #fff; display: inline-block; padding: 0.25em 2em; cursor: pointer; border-radius: 0.4em; background-color: #e78500; background-size: 100% 100%; font-size: 2.6em; border: none; box-shadow: 1px 1px 1px #aaa; background-image: url(data:image/png;base64,); }
    a.button:hover,
    .button:hover { text-shadow: 0 0 0.1em #fff; }

    #header ul li#userMenuSub { position: relative; }
    #header ul li#userMenuSub > a { margin: 0 1em 0 0; }
    #menu #userMenuSub > a span { font-size: 1em; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; display: inline-block; }

    #header ul li#userMenuSub > a:after { content: ""; width: 0; height: 0; border-left: 0.5em solid transparent; border-right: 0.5em solid transparent; border-top: 0.5em solid #E78500; position: relative; top: 0.5em; left: 0.5em; vertical-align: top; display: inline-block; }

    #dashboard .headerWrapper { text-align: left; margin: 1em 0 2em 1em; padding: 0; }
    #dashboard h1 { text-align: left; left: 0; top: 0; position: relative; margin: 0.5em; font-size: 19px; font-weight: normal; }
    .heading span, .heading a { font-size: 1em; }

    #dashboard .report { width: 94em; background-color: #ccc; padding: 1em; position: relative; top: -2px; left: -2px; text-align: left; background-color: #fff; border-top: solid 2px #d7d7ce; border-left: solid 2px #d7d7ce; border-right: solid 2px #d7d7ce; -webkit-border-top-right-radius: 2em; -webkit-border-top-left-radius: 2em; -moz-border-radius-topright: 2em; -moz-border-radius-topleft: 2em; border-top-right-radius: 2em; border-top-left-radius: 2em; }

    #dashboard .report { border: none; border-radius: 0; width: 100%; }

    #dashboard #revenue.report .header .icon { background-position: 0 0; }
    #dashboard .report .header .icon { width: 60px; height: 60px; background-image: url(/content/images/dashboardIconsDemo.png); background-repeat: no-repeat; }
    #dashboard .report .header h2 { font-size: 24px; font-weight: normal; position: absolute; top: 1em; left: 3.5em; margin: 0; }

    #dashboard .report .header .hint { left: 7.8em; top: 4.5em; }
    #dashboard .report .header .hint { font-size: 11px; position: absolute; top: 5em; }
    span.hint { color: #aaa; }

    #dashboard .report .header input[type="text"] { top: 1.25em; color: #606060; font-size: 1.25em; }

    #dashboard .report .header .gauge { position: absolute; top: 1.25em; right: 2em; }
    #dashboard #turnover.report .header .icon { background-position: 0 -240px; }

    #dashboard #yearStarted.report .header .icon { background-position: 0 -305px; }

    #dashboard #salary.report .header .icon { background-position: 0 -60px; }
    #dashboard #revenuePerCapita1.report .header .icon { background-position: 0 -180px; }

    #dashboard #costEffectiveness1.report .header .icon { background-position: 0 -120px; }
    #dashboard .chart a { font-size: 9px; font-weight: bold; display: inline-block; margin: 0 0 0 1em; }
</style>
