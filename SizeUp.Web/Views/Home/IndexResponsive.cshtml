@{
    Layout = "~/Views/Shared/_LayoutResponsive.cshtml";
}

@section head{
    <title>SizeUp - Business Intelligence for All</title>
    <meta content="SizeUp is a free tool that shows how your business compares to competitors, identifies best places to advertise, and maps competitors, customers, and suppliers."
        name="description">
    <link href="@Url.Content("~/Content/thirdParty/toggle-switch.css")?@SizeUp.Core.Web.CacheBuster.Buster" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/thirdParty/animations.css")?@SizeUp.Core.Web.CacheBuster.Buster" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/views/home-responsive.css")?@SizeUp.Core.Web.CacheBuster.Buster" rel="stylesheet" type="text/css" />

    <script type="text/javascript" src="http://api.@SizeUp.Core.Web.WebContext.Current.Domain/js/?apikey=@System.Configuration.ConfigurationManager.AppSettings["SizeUpAPI"]&callback=apiLoaded&@SizeUp.Core.Web.CacheBuster.Buster"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/views/home-ui.js")?@SizeUp.Core.Web.CacheBuster.Buster"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/views/home.js")?@SizeUp.Core.Web.CacheBuster.Buster"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/controls/industrySelector.js")?@SizeUp.Core.Web.CacheBuster.Buster"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/controls/placeSelector.js")?@SizeUp.Core.Web.CacheBuster.Buster"></script>
}


<!-- Login Form Start -->
<div id="login-form" class="jumbotron">
    <div class="text-center">
        <h1 style="font-size:36px;" id="form-header">Business Intelligence for All</h1>
        <p style="color:white;font-weight:100;line-height: 24px;">In 3 steps you will learn how your business stacks up to your competition.</p>
    </div>
    <form id="form" style="margin-bottom:0.5em;">

        <fieldset style="height:265px;margin-top:2.6em;padding-top:.95em;">
            <span style="position: absolute; left: 47em; top: 16em; z-index: 100;" id="noIndustryMatchesMessage" class="info hidden">No industry matched your keyword. Please search again and select from the dropdown.</span>
            <span style="position: absolute; left: 47em; top: 21.6em; z-index: 100;" id="invalidCityMessage" class="info hidden">Please enter a valid city name and select from the dropdown.</span>
            <span style="position: absolute; left: 47em; top: 21.6em; z-index: 100;" id="noValuesEntered" class="info hidden">Please enter a city and industry and select from the dropdowns.</span>

            <div id="industry-container" class="container input-group col-md-7 col-sm-12 col-xs-12">
                <span class="input-group-addon input-group-blue">My industry is</span>
                <input id="searchIndustry" type="text" class="form-control" placeholder="Enter Your Industry">
            </div>

            <div id="location-container" class="container input-group col-md-7 col-sm-12 col-xs-12">
                <span class="input-group-addon input-group-blue">I'm located in</span>
                <input id="searchCommunity" type="text" class="form-control" placeholder="Enter Your City">
                <p id="enteredLocation" class="text-center location hidden">Your city is <a class="changeLocation" href="javascript:void(0);"><span class="locationText"></span>&nbsp;<span class="hint">[change]</span></a></p>
                <p id="detectedLocation" class="text-center location hidden" >We think you are in <a class="changeLocation" href="javascript:void(0);"><span class="locationText"></span>&nbsp;<span class="hint">[change]</span></a></p>
            </div>

            <div id="inline-switch-wrapper" class="clearfix container input-group col-lg-7 col-md-7 col-sm-12 col-xs-12">
                <div id="inline-switch">
                    <div id="exist-copy">
                        <span>I have an existing business</span>
                    </div>
                    <div id="switch-toggle-wrapper">
                        <div class="switch-toggle well">
                            <input id="business" name="view" type="radio" checked="">
                            <label for="business" onclick="swapTextColor(this)"></label>
                            <input id="startup" name="view" type="radio">
                            <label for="startup" onclick="swapTextColor(this)"></label>
                            <a class="btn btn-primary"></a>
                        </div>
                    </div>
                    <div id="start-copy">
                        <span>I want to start a business</span>
                    </div>
                    <div id="continue-col">
                        <button type="button" id="inline-continue" class="btn btn-green btn-primary btn-lg">Continue</button>
                    </div>
                </div>
                <div id="row-switch">
                    <div>
                        <input id="business-sm" name="view" type="radio" checked="">
                        <label for="business-sm">I have an existing business</label>
                    </div>
                    <div>
                        <input id="startup-sm" name="view" type="radio">
                        <label for="startup-sm">I want to start a business</label>
                    </div>
                    <div class="text-center">
                        <br />
                        <button type="button" id="row-continue" class="btn btn-green btn-primary btn-lg">Continue</button>
                    </div>
                </div>
            </div>
        </fieldset>
        <fieldset id="step-two" class="text-center" style="position: absolute; top: 22em; right: -54em; margin-bottom: 0em;">
            <div class="col-lg-10 col-lg-offset-1">
                <div class="col-lg-4 container">
                    <div class="card">
                        <div class="gray-bg face front">
                        </div>
                        <div class="gray-bg face back">
                            <img src="@Url.Content("~/Content/images/gauge-card.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Performance"/>
                            <p>Compare your business to your industry competitors.</p>
                            <a href="#" id="myBusiness" class="btn btn-green btn-primary btn-lg">My Business</a>
                        </div>
                        <p></p>
                    </div>
                </div>
                <div class="col-lg-4 container">
                    <div class="card">
                        <div class="gray-bg face front">
                        </div>
                        <div class="gray-bg face back">
                            <img src="@Url.Content("~/Content/images/compete-card.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Competition"/>
                            <p>Map your customers, competitors, suppliers.</p>
                            <a href="#" id="competing" class="btn btn-green btn-primary btn-lg">Competition</a>
                        </div>
                        <p></p>
                    </div>
                </div>
                <div class="col-lg-4 container">
                    <div class="card">
                        <div class="gray-bg face front">
                        </div>
                        <div class="gray-bg face back">
                            <img src="@Url.Content("~/Content/images/advertise-card.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Advertisement"/>
                            <p>Find best places to target your next advertising campaign.</p>
                            <a href="#" id="advertising" class="btn btn-green btn-primary btn-lg">Advertising</a>
                        </div>
                        <p></p>
                    </div>
                </div>
            </div>
        </fieldset>
    </form>
</div>
<!-- Login Form End -->

<!-- Decisions Start -->
<div id="decisions" class="col-lg-12">
    <div id="section-header" class="row text-center">
        <h2 style="font-weight:400;">SizeUp your business</h2>
        <div class="col-md-8 col-md-offset-2">
            <p class="text-center">Make smarter decisions through data with SizeUp and learn the
                answers to the burning questions that keep every business owner or aspiring entrepreneur
                up at night.</p>
        </div>
    </div>
    <div id="options" class="row">
        <div class="col-wrapper col-lg-4 clearfix">
            <div class="col-lg-4 text-center">
                <img src="@Url.Content("~/Content/images/gauge-home.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="My Business Logo"/><br />
                <a class="left-col btn btn-blue" href="#performance" role="button" style="font-size:12px;">Learn more</a>
            </div>
            <div class="copy text-left col-lg-8">
                <h3 class="text-blue">How do I compare?</h3>
                <p>Find out how you measure up to the rest of the competition in your industry.<a href="#performance">Learn more.</a></p>
                <a class="right-col btn btn-blue" href="#performance" role="button">Learn more</a>
            </div>
        </div>
        <div class="col-wrapper col-lg-4 clearfix">
            <div class="col-lg-4 text-center">
                <img src="@Url.Content("~/Content/images/compete-home.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Competition Logo"/><br />
                <a class="left-col btn btn-blue" href="#competition" role="button" style="font-size:12px;">Learn more</a>
            </div>
            <div class="copy text-left col-lg-8">
                <h3 class="text-blue">Where are my customers?</h3>
                <p>See the locations of potential customers, suppliers, and competitors.<a href="#competition">Learn more.</a></p>
                <a class="right-col btn btn-blue" href="#competition" role="button">Learn more</a>
            </div>
        </div>
        <div class="col-wrapper col-lg-4 clearfix">
            <div class="col-lg-4 text-center">
                <img src="@Url.Content("~/Content/images/advertise-home.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Advertisement Logo"/><br />
                <a class="left-col btn btn-blue" href="#advertisement" role="button" style="font-size:12px;">Learn more</a>
            </div>
            <div class="copy text-left col-lg-8">
                <h3 class="text-blue">Where should I advertise?</h3>
                <p>Invest your advertising budget in the right place. <a href="#advertisement">Learn more.</a></p>
                <a class="right-col btn btn-blue" href="#advertisement" role="button">Learn more</a>
            </div>
        </div>
    </div>
</div>
<!-- Decisions End -->

<!-- Performance Start -->
<div id="performance" class="col-lg-12 clearfix">
    <div id="performance-header" class="col-lg-12">
        <img src="@Url.Content("~/Content/images/gauge-ribbon-home.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Performance Ribbon"/>
        <h2 style="font-weight:400;">How is my business performing?</h2>
        <p>SizeUp shows you how to measure up to the competition in your industry, whether you
            have been in business for years or are just thinking about starting up a business.
        </p>
    </div>

    <div id="slider-column" class="col-lg-6 col-sm-6">
        <div class="container">
            <div class="row">
                <div id="performance-carousel-wrapper" class="col-lg-5">
                    <div id="carousel-example-captions" class="carousel slide" data-ride="carousel">
                        <ol class="carousel-indicators">
                            <li data-target="#carousel-example-captions" data-slide-to="0" class="active"></li>
                            <li data-target="#carousel-example-captions" data-slide-to="1" class=""></li>
                            <li data-target="#carousel-example-captions" data-slide-to="2" class=""></li>
                        </ol>
                        <div class="carousel-inner">
                            <div class="item active">
                                <img src="@Url.Content("~/Content/images/slideRevenue.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Revenue"/>
                            </div>
                            <div class="item">
                                <img src="@Url.Content("~/Content/images/slideYear.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Year"/>
                            </div>
                            <div class="item">
                                <img src="@Url.Content("~/Content/images/slideScorecard.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Year"/>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="slider-sibling-column" class="col-lg-6 col-sm-6">
        <div class="col-lg-1">
            <img src="@Url.Content("~/Content/images/checkmark.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Check Mark"/>
        </div>
        <div class="col-lg-11 right-col">
            <h4>Map the best performing areas around you</h4>
            <p>See where the average business owner in your industry near you is doing best. This
                may help you determine where to expand into a new location, or where to target a
                marketing campaign.</p>
        </div>

        <div class="col-lg-1">
            <img src="@Url.Content("~/Content/images/checkmark.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Check Mark"/>
        </div>
        <div class="col-lg-11 right-col">
            <h4>Get your scorecard</h4>
            <p>Determine what percentage of businesses you are outperforming in your industry. See
                if your score places you in the green or in the red for revenue, employees, salaries,
                and more.</p>
        </div>

        <div class="col-lg-1">
            <img src="@Url.Content("~/Content/images/checkmark.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Check Mark"/>
        </div>
        <div class="col-lg-11 right-col">
            <h4>Get recommendations</h4>
            <p>We don't just show you data, we show you how to look at the information and how to
                make better decisions based on data. Consider us your business intelligence sherpas.
            </p>
        </div>
    </div>
</div>
<div class="blue-footer col-lg-12 text-center clearfix">
    <span>Get started with your comparative market analysis now.</span>
    <a href="#login-form" style="margin-left: 1em;" class="btn btn-green btn-primary btn-lg">
        Start Analysis</a>
</div>
<!-- Performance End -->

<!-- Competition Start -->
<div id="competition" class="col-lg-12 clearfix">
    <div id="competition-header" class="col-lg-12">
        <img src="@Url.Content("~/Content/images/compete-ribbon-home.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Competition Ribbon"/>
        <h2 style="font-weight:400;">Where are my customers, competitors, and suppliers?</h2>
        <p>Find your potential customers, competitors, and suppliers in order to make strategic
            decisions.</p>
    </div>


    <div id="competition-carousel-wrapper" class="col-lg-12">
        <div id="carousel-competition-generic" class="carousel slide">

            <div class="carousel-inner">
                <div class="item active">
                    <div id="map" class="text-center">
                        <img id="animatedDesktop" src="@Url.Content("~/Content/images/sliders-screen-map.png")" style="visibility: hidden;" />
                        <img id="animatedLegend" src="@Url.Content("~/Content/images/sliders-zoom-map.png")" style="visibility: hidden; position: absolute;"/>
                        <div id="animatedFlyer" class="light-gray-bg" style="visibility: hidden;">
                            <img src="@Url.Content("~/Content/images/square-pushpin.png")" /><br />
                            <label>Map your customers, competitors, suppliers.</label>
                            <div class="col-lg-12 text-left" style="margin-bottom: 10px;">
                                <div class="col-lg-2">
                                    <img src="@Url.Content("~/Content/images/slider-dot-blue.png")" alt="Blue Marker"/>
                                </div>
                                <div class="col-lg-10">
                                    <b>Find your customers</b>
                                    <p>Whether you sell to businesses or consumers, SizeUp can show you where to find your
                                        customers.</p>
                                </div>
                            </div>
                            <div class="col-lg-12 text-left" style="margin-bottom: 10px;">
                                <div class="col-lg-2">
                                    <img src="@Url.Content("~/Content/images/slider-dot-red.png")" alt="Blue Marker"/>
                                </div>
                                <div class="col-lg-10">
                                    <b>Identify your competition</b>
                                    <p>Learn who your competitors are and where they are located.</p>
                                </div>
                            </div>
                            <div class="col-lg-12 text-left" style="margin-bottom: 10px;">
                                <div class="col-lg-2">
                                    <img src="@Url.Content("~/Content/images/slider-dot-green.png")" alt="Blue Marker"/>
                                </div>
                                <div class="col-lg-10">
                                    <b>Map your supply chain</b>
                                    <p>Discover where your closest suppliers are located. Are you in the best strategic
                                        location for your business?</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="item">
                    <div id="expenditures" class="text-center">
                        <img id="animatedDesktop" src="@Url.Content("~/Content/images/desktop.png")" />
                        <img id="animatedLegend" src="@Url.Content("~/Content/images/legend2.png")" style="position: absolute;
                            border: 7px solid white; border-radius: 6.5em; box-shadow: 0 3px 8px rgba(0, 0, 0, .8);
                            -webkit-box-shadow: 0 3px 8px rgba(0, 0, 0, .8); -moz-box-shadow: 0 3px 8px rgba(0, 0, 0, .8);" />
                        @*<img id="animatedFlyer" src="@Url.Content("~/Content/images/flyer.png")" style="position: absolute;
                            border-radius: .5em; -webkit-box-shadow: 3px 4px 5px 0px rgba(50, 50, 50, 0.75);
                            -moz-box-shadow: 3px 4px 5px 0px rgba(50, 50, 50, 0.75); box-shadow: 3px 4px 5px 0px rgba(50, 50, 50, 0.75);" />*@
                        <div id="animatedFlyer" class="light-gray-bg">
                            <img src="@Url.Content("~/Content/images/square-layers.png")" /><br />
                            <label>See where and how much customers spend.</label>
                            <div class="col-lg-12 text-left" style="margin-bottom: 10px;">
                                <div class="col-lg-2">
                                    <img src="@Url.Content("~/Content/images/check-blue.png")" alt="Blue Marker"/>
                                </div>
                                <div class="col-lg-10">
                                    <b>Choose your category</b>
                                    <p>Choose from hundreds of consumer expenditure categories, from men's sweaters to magazines.
                                    </p>
                                </div>
                            </div>
                            <div class="col-lg-12 text-left" style="margin-bottom: 10px;">
                                <div class="col-lg-2">
                                    <img src="@Url.Content("~/Content/images/check-blue.png")" alt="Blue Marker"/>
                                </div>
                                <div class="col-lg-10">
                                    <b>Map spending patterns</b>
                                    <p>SizeUp can map consumer expenditure patterns in your neighborhood and beyond.
                                    </p>
                                </div>
                            </div>
                            <div class="col-lg-12 text-left" style="margin-bottom: 10px;">
                                <div class="col-lg-2">
                                    <img src="@Url.Content("~/Content/images/check-blue.png")" alt="Blue Marker"/>
                                </div>
                                <div class="col-lg-10">
                                    <b>Total vs average household</b>
                                    <p>Find where the total consumer spending is highest for your category, or see where
                                        the average household spends the most.</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Controls -->
            <a class="left carousel-control" href="#carousel-competition-generic" data-slide="prev"
                style="visibility: hidden;">
                <span>
                    <img src="@Url.Content("~/Content/images/slider-nav-prev.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Prev"/></span>
            </a>
            <a class="right carousel-control" href="#carousel-competition-generic" data-slide="next"
                style="visibility: hidden;">
                <span>
                    <img src="@Url.Content("~/Content/images/slider-nav-next.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Next"/></span>
            </a>
        </div>
    </div>
</div>
<div class="gradient gray-bg col-lg-12 text-center clearfix" style="margin-top: 0;
    padding: 4em 0; border-bottom: none; border-top: none; border-left: 2px solid #e2e2e2;
    border-right: 2px solid #e2e2e2;">
    <div class="col-lg-6 col-md-6 col-sm-6">
        <img  class="text-center" src="@Url.Content("~/Content/images/fact-1.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Fact"/>
        <div class="col-lg-12 col-md-12 col-sm-12">
            <h4 style="font-size: 20px;" class="text-left">Businesses want to outperform the competition
            </h4>
            <p class="text-left">SizeUp enables businesses to perform side-by-side, apples-to-apples
                competitiveness comparison of their business to all other competitors in their industry
                from the local. <a href="#">Learn more.</a></p>
        </div>
    </div>
    <div class="col-lg-6 col-md-6 col-sm-6">
        <img  class="text-center" src="@Url.Content("~/Content/images/fact-2.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Fact"/>
        <div class="col-lg-12 col-md-12 col-sm-12">
            <h4 style="font-size: 20px;" class="text-left">Businesses want to know what they don't
                know</h4>
            <p class="text-left">Everyone is limited by not knowing what they don't know. SizeUp
                provides information to businesses that they don't know they need, but which can
                make. <a href="#">Learn more.</a></p>
        </div>
    </div>
</div>
<div id="gauge" class="col-lg-12 text-center clearfix" style="color: #ffffff; margin-top: 0; padding-top: 1em; padding-bottom: 1em; background-color: #005687; font-size: 18px; border-top: none;">
    <span>Find out where your customers, competitors, and suppliers are.</span><a href="#login-form" style="margin-left: 1em;" class="btn btn-green btn-primary btn-lg">Start Analysis</a>
</div>
<!-- Competition End -->


<div id="advertisement" class="col-lg-12 clearfix" style="padding-bottom: 2em; border-bottom: none;">
    <div class="col-lg-12" style="margin-bottom: 2em;">
        <img  class="text-center" src="@Url.Content("~/Content/images/advertise-ribbon-home.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="Advertisement"/>
        <h2 style="font-weight:400;">Where should I advertise?</h2>
        <p style="color: #005687; font-size: 1.2em;">SizeUp can help you find the best places
