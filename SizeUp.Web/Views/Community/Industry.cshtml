@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section head{
<link href="@Url.Content("~/Content/views/community/find.css")?@SizeUp.Core.Web.CacheBuster.Buster" rel="stylesheet" type="text/css" />
<title>Find Industries by Category in @ViewBag.CurrentInfo.CurrentPlace.City.Name, @ViewBag.CurrentInfo.CurrentPlace.State.Abbreviation ( @ViewBag.CurrentInfo.CurrentPlace.County.Name Cnty) | SizeUp</title>
<meta name="keywords" content="" />
<meta name="description" content="Find industries by category in @ViewBag.CurrentInfo.CurrentPlace.City.Name, @ViewBag.CurrentInfo.CurrentPlace.State.Abbreviation from SizeUp list of businesses or expand your search to neighboring cities in @ViewBag.CurrentInfo.CurrentPlace.State.Name" />

}

<div id="community" class="industry">
    <h1>Industries in @ViewBag.CurrentInfo.CurrentPlace.City.Name, @ViewBag.CurrentInfo.CurrentPlace.State.Name (@ViewBag.CurrentInfo.CurrentPlace.County.Name County - @ViewBag.CurrentInfo.CurrentPlace.City.TypeName)</h1>
    <a href="/community/@ViewBag.CurrentInfo.CurrentPlace.State.SEOKey/@ViewBag.CurrentInfo.CurrentPlace.County.SEOKey/@ViewBag.CurrentInfo.CurrentPlace.City.SEOKey" class="inverse details">View Detailed Community Report</a>


    @foreach(var col in ViewBag.Industries)
    {
    <div class="col">
        @foreach (List<SizeUp.Core.DataLayer.Models.Industry> list in col)
        {
        <h3>@list[0].ParentName</h3>
        <ul>
            @foreach(var item in list)
            {
            <li>
                <a href="/community/@ViewBag.CurrentInfo.CurrentPlace.State.SEOKey/@ViewBag.CurrentInfo.CurrentPlace.County.SEOKey/@ViewBag.CurrentInfo.CurrentPlace.City.SEOKey/@item.SEOKey">@item.Name </a>
            </li>
            }
        </ul>
        }
    </div>

    }

</div>





