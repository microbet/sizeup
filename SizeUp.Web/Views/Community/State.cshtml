@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section head{
<link href="@Url.Content("~/Content/views/community/find.css")?@SizeUp.Core.Web.CacheBuster.Buster" rel="stylesheet" type="text/css" />
<meta name="robots" content="noindex" />
    <title>Find List of State | SizeUp</title>
<meta name="description" content="Find 2016 demographics, populations &amp; household spending data by state from SizeUp or expand your search to business categories." />

}

<div id="community" class="state">
    <h1>Communities by State</h1>
 
    @foreach(var col in ViewBag.States)
    {
    <div class="col">
        <ul>
            @foreach(var item in col)
            {
            <li>
                <a href="/find-community/@item.SEOKey/" class="inverse">@item.Name</a>
            </li>
            }
        </ul>
    </div>
    }

</div>





