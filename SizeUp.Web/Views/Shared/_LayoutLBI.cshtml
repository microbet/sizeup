<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0"/> <!--320-->
    <meta charset="utf-8" />
    <link href="@Url.Content("~/Content/LBI/reset.css")?@SizeUp.Core.Web.CacheBuster.Buster" rel="stylesheet" type="text/css" />
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link rel="icon" type="image/png" href="@Url.Content("~/content/LBI/images/favicon.ico")?@SizeUp.Core.Web.CacheBuster.Buster" />
    <link href="@Url.Content("~/Content/LBI/layout-responsive.css")?@SizeUp.Core.Web.CacheBuster.Buster" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/LBI/print.css")?@SizeUp.Core.Web.CacheBuster.Buster" rel="stylesheet" type="text/css" media="print" />

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js" type="text/javascript"></script>
    <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.8.18/jquery-ui.min.js" type="text/javascript"></script>
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js" type="text/javascript"></script>
    @Html.Partial("Analytics")

    <link href="@Url.Content("~/Content/LBI/home-responsive.css")?@SizeUp.Core.Web.CacheBuster.Buster" rel="stylesheet" type="text/css" />
    <script type="text/javascript" src="@Url.Content("~/Scripts/LBI/home-ui.js")?@SizeUp.Core.Web.CacheBuster.Buster"></script>
    @RenderSection("head",false)
</head>
<body>
    <div id="header">
        <div class="container">
            <nav id="home-navbar" class="navbar" role="navigation">
                <a id="logo" class="pull-left col-xs-6 col-sm-4 col-md-3" href="/lbi" style="padding-left:0;">
                    <img class="img-responsive" src="@Url.Content("~/Content/LBI/images/SizeUpLBI-logo.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="SizeUp Logo"/>
                </a>
                <a href="#" onclick="$('html, body').animate({scrollTop: $('#contact').offset().top }, 1000);" id="login-btn" class="col-xs-4 col-sm-4 col-md-2 col-lg-1 btn btn-green btn-primary btn-lg pull-right">Contact Us</a>
                
            </nav>
        </div>
    </div>
    <div class="container">
        @RenderBody()
    </div>
    <script type="text/javascript"> 
        $(document).ready(function(){
            checkWidth();
        });
    </script>
</body>
</html>
