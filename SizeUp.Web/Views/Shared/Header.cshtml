<a id="logo" href="/"><img src="@Url.Content("~/Content/images/sizeup.png")?@SizeUp.Core.Web.CacheBuster.Buster" alt="SizeUp Logo"/></a>
@if(!ViewBag.Header.HideMenu){
<div id="menu" class="@(SizeUp.Core.Identity.Identity.CurrentUser!=null ? "signinIn" : "")  ">
<ul>
@if (!ViewBag.Header.HideNavigation && ViewBag.CurrentInfo.CurrentPlace!= null && ViewBag.CurrentInfo.CurrentPlace.Id != null && ViewBag.CurrentInfo.CurrentIndustry != null)
{
if(ViewBag.Header.ActiveTab == SizeUp.Web.Models.NavItems.Dashboard){
<li><a class="disabled">My Business</a></li>
}else{
<li><a href="/dashboard/@ViewBag.CurrentInfo.CurrentPlace.State.SEOKey/@ViewBag.CurrentInfo.CurrentPlace.County.SEOKey/@ViewBag.CurrentInfo.CurrentPlace.City.SEOKey/@ViewBag.CurrentInfo.CurrentIndustry.SEOKey/@ViewBag.CurrentInfo.CurrentBusinessStatus">My Business</a></li>
}
<li><span>|</span></li>
if(ViewBag.Header.ActiveTab == SizeUp.Web.Models.NavItems.Competition){
<li><a class="disabled">Competition</a></li>
}else{
<li><a href="/competition/@ViewBag.CurrentInfo.CurrentPlace.State.SEOKey/@ViewBag.CurrentInfo.CurrentPlace.County.SEOKey/@ViewBag.CurrentInfo.CurrentPlace.City.SEOKey/@ViewBag.CurrentInfo.CurrentIndustry.SEOKey">Competition</a></li>
}
<li><span>|</span></li>
if(ViewBag.Header.ActiveTab == SizeUp.Web.Models.NavItems.Advertising){
<li><a class="disabled">Advertising</a></li>
}else{
<li><a href="/advertising/@ViewBag.CurrentInfo.CurrentPlace.State.SEOKey/@ViewBag.CurrentInfo.CurrentPlace.County.SEOKey/@ViewBag.CurrentInfo.CurrentPlace.City.SEOKey/@ViewBag.CurrentInfo.CurrentIndustry.SEOKey">Advertising</a></li>
}



}
@if(SizeUp.Core.Identity.Identity.CurrentUser==null){
<li><a href="javascript:sizeup.core.profile.signIn();">Sign In</a></li>
}else{
<li id="userMenu">
    <a href="javascript:void(0);"><span>@SizeUp.Core.Identity.Identity.CurrentUser.DisplayName</span></a>
    <ul>
        <li><a href="/user/profile">Account Settings</a></li>
        <li><a href="javascript:sizeup.core.profile.signOut();">Sign Out</a></li>
    </ul>
</li>
}
</ul>
</div>
}

