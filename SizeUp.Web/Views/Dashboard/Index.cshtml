@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section head{
<title>Business Listings</title>
<link href="@Url.Content("~/Content/views/dashboard/index.css")" rel="stylesheet" type="text/css" />

<script type="text/javascript" src="http://maps.google.com/maps/api/js?key=@System.Configuration.ConfigurationManager.AppSettings["GoogleAPI"]&sensor=false"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/thirdParty/raphael-min.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/core/data.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/util/numbers/format.js")"></script>

<script type="text/javascript" src="@Url.Content("~/Scripts/controls/gauge.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/charts/barchart.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/controls/contentExpander.js")"></script>

<script type="text/javascript" src="@Url.Content("~/Scripts/views/dashboard/revenue.js")"></script>


<script type="text/javascript" src="@Url.Content("~/Scripts/views/dashboard/dashboard.js")"></script>

}


<div id="dashboard">
<h1>Pharmacies is San Francisco, CA</h1>

    <div id="revenue" class="report">
        <div class="header">
            <div class="icon"></div>
            <h2>Revenue</h2>
            <span class="hint hidden">Annual revenue of your business</span>
            <input type="text" class="valueBox" />
            <span class="prompt hidden">Please enter <br />your annual revenue</span>
            <div class="gauge hidden"></div>
            <a href="javascript:void(0)" class="button medium runReport hidden">Run Report</a>
        </div>
        <div class="body hidden">
            <div class="loading">
                <p class="title">Average Revenue Report</p>
                <img src="@Url.Content("~/content/images/Loading.gif")" alt="Loading" />
                <p class="message">Generating Report</p>
            </div>
            <div class="reportContainer hidden">
                <div class="reportContent">
                    <h3>How you size up</h3>
                    <p>Your business generates less revenue than 99% of businesses in your industry in the nation.</p>
                    <div class="chart">
                        <a href="#">Hide Variations by Geography</a>
                        <a href="#">View Data Table </a>
                        <div class="container"></div>
                    </div>
                    <ul class="links">
                        <li>
                            <a class="source" href="javascript:void(0);" data-toggleText="Hide">Source</a>
                        </li>
                    </ul>
                    <div class="sourceContent hidden" data-expandedClass="expanded">
                        <p>Average revenue data for communities on SizeUp is aggregated from data on individual businesses,
                             which comes from hundreds of data sources including, but not limited to, IRS records, county 
                             courthouse filings, business publications, and corporate annual reports.
                        </p>
                    </div>
                    <div class="map"></div>
                </div>
                <div class="reportSidebar">
                    <div class="section">
                        <h4>Considerations</h4>
                        <p>Average annual revenue measures the income that the average business 
                            in the area generates from selling its goods and services. You can use this 
                            figure as a benchmark to determine where you should be setting goals for your 
                            own performance.</p>

                        <p>The map shows the areas with the highest average revenue for businesses in your industry.
                           These are areas you may want to target a marketing campaign or open a future 
                           business location.</p>
                    </div>
                    <div class="section">
                        <h4>Resources</h4>
                        <p>Apply for a small business loan from <a href="http://www.ondeckcapital.com/size-up?utm_source=sizeup&utm_medium=sizeup&utm_campaign=sizeup"  target="_blank">On Deck</a>.</p>
		                <p>Connect with investors and receive a peer-to-peer loan from <a href="http://www.shareasale.com/r.cfm?b=262903&u=592999&m=29606&urllink=&afftrack=" target="_blank">Prosper</a>.</p>
		                <p>Get business assistance consulting from <a href="http://www.score.org" target="_blank">SCORE</a> or <a href="http://www.asbdc-us.org/" target="_blank">the Association of Small Business Development Centers</a>.</p>
                        <p>Track revenue and expenses with <a href="http://quickbooks.intuit.com/" target="_blank">Quickbooks</a>, <a href="https://indinero.com/" target="_blank">inDinero</a>, or <a href="http://waveaccounting.com/" target="_blank">Wave Accounting</a>.</p>
		                <p>Develop a business plan and create forecasts using <a href="http://www.enloop.com" target="_blank">Enloop</a>.</p>       
		                <p>Easily visualize QuickBooks data with graphics using <a href="http://www.mybizhomepage.com/" target="_blank">MyBizHomepage</a>.</p> 
		                <p>Expand your business through franchising with <a href="http://www.bibbygroup.com/franchise-consultants.htm" target="_blank">the Bibby Group</a> or <a href="http://www.ifranchisegroup.com" target="_blank">the iFranchise Group</a>.</p>
		                <p>Expand your search engine presence with <a href="http://www.google.com/placesforbusiness" target="_blank">Google Places</a>.</p>
		                <p>Grow your business online with a website from <a href="http://www.dpbolvw.net/click-5633483-10378406" target="_blank">GoDaddy</a><img src="http://www.awltovhc.com/image-5633483-10378406" width="1" height="1" border="0"/>.</p>
		                <p>Get your free credit score from <a href="http://www.creditkarma.com/" target="_blank">Credit Karma</a> or <a href="http://equifax.com/freetrial" target="_blank">Equifax</a>.</p>
		                <p>Discover what your business is worth with <a href="https://www.bizequity.com/" target="_blank">BizEquity</a>.</p>
		                <p>Sell your business at <a href="http://www.bizbuysell.com/" target="_blank">BizBuySell</a>.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="yearStarted" class="report">
        <div class="header">
            <div class="icon"></div>
            <h2>Year Started</h2>
            <span class="hint">The year your business was started</span>
            <input type="text" />
            <span class="prompt">Please enter <br />the year your business started</span>
        </div>
        <div class="body hidden"></div>
    </div>

    <div id="salary" class="report">
        <div class="header">
            <div class="icon"></div>
            <h2>Salary</h2>
            <span class="hint">Average annual worker salary</span>
            <input type="text" />
            <span class="prompt">Please enter <br />your average employee salary</span>
        </div>
        <div class="body hidden"></div>
    </div>

    <div id="employees" class="report">
        <div class="header">
            <div class="icon"></div>
            <h2>Employees</h2>
            <span class="hint">Number of full-time equivalent employees</span>
            <input type="text" />
            <span class="prompt">Please enter <br />how many employees you have</span>
        </div>
        <div class="body hidden"></div>
    </div>

    <div id="costEffectiveness" class="report">
        <div class="header">
            <div class="icon"></div>
            <h2>Cost Effectiveness</h2>
            <span class="hint">Revenue per employee divided by worker salary</span>
            <input type="text" />
            <span class="prompt">Please provide revenue, number of employees and salary to see this report</span>
        </div>
        <div class="body hidden"></div>
    </div>

    <div id="revenuePerCapita" class="report">
        <div class="header">
            <div class="icon"></div>
            <h2>Revenue Per Capita</h2>
            <span class="hint">Revenue generated per community resident</span>
            <input type="text" />
        </div>
        <div class="body hidden"></div>
    </div>

    <div id="turnover" class="report">
        <div class="header">
            <div class="icon"></div>
            <h2>Local Turnover</h2>
            <span class="hint">Percentage of workers newly hired</span>
            <input type="text" />
        </div>
        <div class="body hidden"></div>
    </div>

    <div id="healthcareCost" class="report">
        <div class="header">
            <div class="icon"></div>
            <h2>Healthcare Cost</h2>
            <span class="hint">Annual healthcare premium per employee</span>
            <input type="text" />
            <span class="prompt">Please enter your annual healthcare premium per employee</span>
        </div>
        <div class="body hidden"></div>
    </div>

    <div id="workersComp" class="report">
        <div class="header">
            <div class="icon"></div>
            <h2>Workers' Compensation</h2>
            <span class="hint">Premium per employee (per $100 of payroll)</span>
            <input type="text" />
            <span class="prompt">Please enter premium per employee<br/>(per $100 of payroll)</span>
        </div>
        <div class="body hidden"></div>
    </div>

    <p class="info">
        <span>Please <a href="/user/signin">Sign In</a> to see more reports.</span>
        <span>Don't have an account? Complete a <a href="/user/register">free registration</a>.</span>
    </p>
</div>


@section scripts{

<script type="text/javascript">
    $(document).ready(function () {
        var dirver = new sizeup.views.dashboard.dashboard();
    });
</script>
}