{
 "title": "This report provided by your friendly neighborhood business banker",
 "query": {
  "template": "What are the ${granularity} places in ${area}, ranked by ${ranking_metric}, given ${filters}?",
  "granularity": "ZipCode",
  "area": {
    "template": "within ${distance} of ${place}",
    "distance": "11",
    "place": {
      "state": "texas",
      "county": "tarrant",
      "city": "fort-worth-city"
    }
  },
  "ranking_metric": {
    "template": "${kpi} in ${industry} (${order})",
    "kpi": "revenuePerCapita",
    "industry": "shoe-and-footwear-stores",
    "order": "desc"
  },
  "filter": {
    "totalRevenue": {
      "min": "100000",
      "max": "100000000000"
    },
    "averageRevenue": {
      "min": "50000",
      "max": "50000000"
    },
    "totalEmployees": {
      "min": "10",
      "max": "1000000"
    },
    "revenuePerCapita": {
      "min": "5",
      "max": "15000"
    },
    "householdIncome": {
      "min": "10000",
      "max": "250000"
    },
    "householdExpenditures": {
      "min": "10000",
      "max": "250000"
    },
    "medianAge": {
      "min": "5",
      "max": "80"
    },
    "highSchoolOrHigher": {
      "min": "2"
    },
    "whiteCollarWorkers": {
      "min": "4"
    },
    "bachelorsDegreeOrHigher": {
      "min": "1"
    }
  }
 }
}
